{
  "$schema": "vscode://schemas/color-theme",
  "name": "DankShell Dark - Generated",
  "type": "dark",
  "colors": {
    "editor.background": "#151218",
    "editor.foreground": "#e7e0e8",
    "editor.lineNumberColor": "#cbc4cf",
    "editorLineNumber.foreground": "#cbc4cf",
    "editorLineNumber.activeForeground": "#e7e0e8",
    "editorCursor.foreground": "#d5bbfc",
    "editor.selectionBackground": "#513c7360",
    "editor.wordHighlightBackground": "#4b435740",
    "editor.lineHighlightBackground": "#211e2440",
    "editorWhitespace.foreground": "#49454e",
    "editorIndentGuide.foreground": "#49454e40",
    "editorError.foreground": "#ffb4ab",
    "editorWarning.foreground": "#ffd9e0",
    "editorInfo.foreground": "#cec2db",
    "editorHint.foreground": "#f1b7c3",
    "activityBar.background": "#1d1a20",
    "activityBar.foreground": "#cbc4cf",
    "activityBar.activeBorder": "#d5bbfc",
    "activityBarBadge.background": "#d5bbfc",
    "activityBarBadge.foreground": "#3a255b",
    "sideBar.background": "#1d1a20",
    "sideBar.foreground": "#cbc4cf",
    "sideBar.border": "#49454e20",
    "sideBarTitle.foreground": "#e7e0e8",
    "sideBarSectionHeader.background": "#211e24",
    "sideBarSectionHeader.foreground": "#e7e0e8",
    "list.focusBackground": "#211e24",
    "list.hoverBackground": "#211e2440",
    "list.activeSelectionBackground": "#513c73",
    "list.activeSelectionForeground": "#ecdcff",
    "list.inactiveSelectionBackground": "#2c292f",
    "statusBar.background": "#37333a",
    "statusBar.foreground": "#e7e0e8",
    "statusBar.border": "#49454e20",
    "statusBar.debuggingBackground": "#93000a",
    "statusBar.debuggingForeground": "#ffdad6",
    "statusBarItem.hoverBackground": "#37333a80",
    "titleBar.activeBackground": "#0f0d12",
    "titleBar.activeForeground": "#e7e0e8",
    "titleBar.inactiveBackground": "#0f0d1280",
    "titleBar.inactiveForeground": "#cbc4cf",
    "commandCenter.background": "#211e24",
    "commandCenter.foreground": "#e7e0e8",
    "commandCenter.border": "#49454e40",
    "tab.activeBackground": "#211e24",
    "tab.activeForeground": "#e7e0e8",
    "tab.activeBorder": "#d5bbfc",
    "tab.inactiveBackground": "#1d1a20",
    "tab.inactiveForeground": "#cbc4cf",
    "tab.hoverBackground": "#211e2440",
    "tab.border": "#49454e20",
    "tab.unfocusedActiveBackground": "#211e2460",
    "tab.unfocusedActiveForeground": "#cbc4cf",
    "input.background": "#2c292f",
    "input.foreground": "#e7e0e8",
    "input.border": "#49454e40",
    "input.placeholderForeground": "#cbc4cf",
    "inputOption.activeBackground": "#513c73",
    "inputOption.activeForeground": "#ecdcff",
    "button.background": "#d5bbfc",
    "button.foreground": "#3a255b",
    "button.hoverBackground": "#d5bbfcdd",
    "button.secondaryBackground": "#cec2db",
    "button.secondaryForeground": "#352d40",
    "button.secondaryHoverBackground": "#cec2dbdd",
    "dropdown.background": "#2c292f",
    "dropdown.foreground": "#e7e0e8",
    "dropdown.border": "#49454e40",
    "progressBar.background": "#d5bbfc",
    "scrollbar.shadow": "#00000040",
    "scrollbarSlider.background": "#49454e40",
    "scrollbarSlider.hoverBackground": "#49454e60",
    "scrollbarSlider.activeBackground": "#49454e80",
    "editorGroup.border": "#49454e20",
    "editorGroup.dropBackground": "#513c7340",
    "editorGroupHeader.tabsBackground": "#1d1a20",
    "editorGroupHeader.tabsBorder": "#49454e20",
    "diffEditor.insertedTextBackground": "#643b4540",
    "diffEditor.removedTextBackground": "#93000a40",
    "diffEditor.diagonalFill": "#49454e20",
    "minimap.background": "#1d1a20",
    "minimap.selectionHighlight": "#513c7360",
    "minimap.errorHighlight": "#ffb4ab60",
    "minimap.warningHighlight": "#ffd9e060",
    "peekView.border": "#49454e",
    "peekViewEditor.background": "#211e2480",
    "peekViewEditor.matchHighlightBackground": "#4b435760",
    "peekViewResult.background": "#1d1a20",
    "peekViewResult.matchHighlightBackground": "#4b435740",
    "peekViewResult.selectionBackground": "#513c7340",
    "widget.shadow": "#00000040",
    "problemsErrorIcon.foreground": "#ffb4ab",
    "problemsWarningIcon.foreground": "#ffd9e0",
    "problemsInfoIcon.foreground": "#cec2db",
    "debugExceptionWidget.background": "#93000a",
    "debugExceptionWidget.border": "#ffb4ab",
    "debugToolBar.background": "#211e24",
    "debugToolBar.border": "#49454e20",
    "breadcrumb.background": "#151218",
    "breadcrumb.foreground": "#cbc4cf",
    "breadcrumb.focusForeground": "#e7e0e8",
    "breadcrumb.activeSelectionForeground": "#d5bbfc"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "foreground": "#cbc4cf",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#f1b7c3"
      }
    },
    {
      "name": "String Escape",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#cec2db"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#cec2db"
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#cec2db"
      }
    },
    {
      "name": "Built-in Constant",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#cec2db"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#e7e0e8"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#d5bbfc"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage"],
      "settings": {
        "foreground": "#d5bbfc"
      }
    },
    {
      "name": "Storage Type",
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#d5bbfc"
      }
    },
    {
      "name": "Class name",
      "scope": ["entity.name.class"],
      "settings": {
        "fontStyle": "",
        "foreground": "#cec2db"
      }
    },
    {
      "name": "Inherited class",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#cec2db"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function"],
      "settings": {
        "fontStyle": "",
        "foreground": "#cec2db"
      }
    },
    {
      "name": "Function argument",
      "scope": ["variable.parameter"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#cbc4cf"
      }
    },
    {
      "name": "Tag name",
      "scope": ["entity.name.tag"],
      "settings": {
        "fontStyle": "",
        "foreground": "#d5bbfc"
      }
    },
    {
      "name": "Tag attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "fontStyle": "",
        "foreground": "#cec2db"
      }
    },
    {
      "name": "Library function",
      "scope": ["support.function"],
      "settings": {
        "fontStyle": "",
        "foreground": "#cec2db"
      }
    },
    {
      "name": "Library constant",
      "scope": ["support.constant"],
      "settings": {
        "fontStyle": "",
        "foreground": "#cec2db"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "fontStyle": "",
        "foreground": "#cec2db"
      }
    },
    {
      "name": "Library variable",
      "scope": ["support.other.variable"],
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "background": "#ffb4ab",
        "fontStyle": "",
        "foreground": "#690005"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "background": "#93000a",
        "foreground": "#ffdad6"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#cec2db"
      }
    },
    {
      "name": "Markup: Strong",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#cec2db"
      }
    },
    {
      "name": "Markup: Underline",
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": ["markup.heading"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#d5bbfc"
      }
    },
    {
      "name": "Markup: Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#cec2db"
      }
    }
  ]
}
