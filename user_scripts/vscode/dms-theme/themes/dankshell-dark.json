{
  "$schema": "vscode://schemas/color-theme",
  "name": "DankShell Dark - Generated",
  "type": "dark",
  "colors": {
    "editor.background": "#12140e",
    "editor.foreground": "#e2e3d8",
    "editor.lineNumberColor": "#c5c8ba",
    "editorLineNumber.foreground": "#c5c8ba",
    "editorLineNumber.activeForeground": "#e2e3d8",
    "editorCursor.foreground": "#b2d189",
    "editor.selectionBackground": "#354e1560",
    "editor.wordHighlightBackground": "#404a3340",
    "editor.lineHighlightBackground": "#1e201a40",
    "editorWhitespace.foreground": "#44483d",
    "editorIndentGuide.foreground": "#44483d40",
    "editorError.foreground": "#ffb4ab",
    "editorWarning.foreground": "#bcece7",
    "editorInfo.foreground": "#c0cbad",
    "editorHint.foreground": "#a0d0cb",
    "activityBar.background": "#1a1c16",
    "activityBar.foreground": "#c5c8ba",
    "activityBar.activeBorder": "#b2d189",
    "activityBarBadge.background": "#b2d189",
    "activityBarBadge.foreground": "#1f3701",
    "sideBar.background": "#1a1c16",
    "sideBar.foreground": "#c5c8ba",
    "sideBar.border": "#44483d20",
    "sideBarTitle.foreground": "#e2e3d8",
    "sideBarSectionHeader.background": "#1e201a",
    "sideBarSectionHeader.foreground": "#e2e3d8",
    "list.focusBackground": "#1e201a",
    "list.hoverBackground": "#1e201a40",
    "list.activeSelectionBackground": "#354e15",
    "list.activeSelectionForeground": "#cdeda3",
    "list.inactiveSelectionBackground": "#282b24",
    "statusBar.background": "#33362e",
    "statusBar.foreground": "#e2e3d8",
    "statusBar.border": "#44483d20",
    "statusBar.debuggingBackground": "#93000a",
    "statusBar.debuggingForeground": "#ffdad6",
    "statusBarItem.hoverBackground": "#33362e80",
    "titleBar.activeBackground": "#0d0f09",
    "titleBar.activeForeground": "#e2e3d8",
    "titleBar.inactiveBackground": "#0d0f0980",
    "titleBar.inactiveForeground": "#c5c8ba",
    "commandCenter.background": "#1e201a",
    "commandCenter.foreground": "#e2e3d8",
    "commandCenter.border": "#44483d40",
    "tab.activeBackground": "#1e201a",
    "tab.activeForeground": "#e2e3d8",
    "tab.activeBorder": "#b2d189",
    "tab.inactiveBackground": "#1a1c16",
    "tab.inactiveForeground": "#c5c8ba",
    "tab.hoverBackground": "#1e201a40",
    "tab.border": "#44483d20",
    "tab.unfocusedActiveBackground": "#1e201a60",
    "tab.unfocusedActiveForeground": "#c5c8ba",
    "input.background": "#282b24",
    "input.foreground": "#e2e3d8",
    "input.border": "#44483d40",
    "input.placeholderForeground": "#c5c8ba",
    "inputOption.activeBackground": "#354e15",
    "inputOption.activeForeground": "#cdeda3",
    "button.background": "#b2d189",
    "button.foreground": "#1f3701",
    "button.hoverBackground": "#b2d189dd",
    "button.secondaryBackground": "#c0cbad",
    "button.secondaryForeground": "#2a331e",
    "button.secondaryHoverBackground": "#c0cbaddd",
    "dropdown.background": "#282b24",
    "dropdown.foreground": "#e2e3d8",
    "dropdown.border": "#44483d40",
    "progressBar.background": "#b2d189",
    "scrollbar.shadow": "#00000040",
    "scrollbarSlider.background": "#44483d40",
    "scrollbarSlider.hoverBackground": "#44483d60",
    "scrollbarSlider.activeBackground": "#44483d80",
    "editorGroup.border": "#44483d20",
    "editorGroup.dropBackground": "#354e1540",
    "editorGroupHeader.tabsBackground": "#1a1c16",
    "editorGroupHeader.tabsBorder": "#44483d20",
    "diffEditor.insertedTextBackground": "#1f4e4b40",
    "diffEditor.removedTextBackground": "#93000a40",
    "diffEditor.diagonalFill": "#44483d20",
    "minimap.background": "#1a1c16",
    "minimap.selectionHighlight": "#354e1560",
    "minimap.errorHighlight": "#ffb4ab60",
    "minimap.warningHighlight": "#bcece760",
    "peekView.border": "#44483d",
    "peekViewEditor.background": "#1e201a80",
    "peekViewEditor.matchHighlightBackground": "#404a3360",
    "peekViewResult.background": "#1a1c16",
    "peekViewResult.matchHighlightBackground": "#404a3340",
    "peekViewResult.selectionBackground": "#354e1540",
    "widget.shadow": "#00000040",
    "problemsErrorIcon.foreground": "#ffb4ab",
    "problemsWarningIcon.foreground": "#bcece7",
    "problemsInfoIcon.foreground": "#c0cbad",
    "debugExceptionWidget.background": "#93000a",
    "debugExceptionWidget.border": "#ffb4ab",
    "debugToolBar.background": "#1e201a",
    "debugToolBar.border": "#44483d20",
    "breadcrumb.background": "#12140e",
    "breadcrumb.foreground": "#c5c8ba",
    "breadcrumb.focusForeground": "#e2e3d8",
    "breadcrumb.activeSelectionForeground": "#b2d189"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "foreground": "#c5c8ba",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#a0d0cb"
      }
    },
    {
      "name": "String Escape",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#c0cbad"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#c0cbad"
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#c0cbad"
      }
    },
    {
      "name": "Built-in Constant",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#c0cbad"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#e2e3d8"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#b2d189"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage"],
      "settings": {
        "foreground": "#b2d189"
      }
    },
    {
      "name": "Storage Type",
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#b2d189"
      }
    },
    {
      "name": "Class name",
      "scope": ["entity.name.class"],
      "settings": {
        "fontStyle": "",
        "foreground": "#c0cbad"
      }
    },
    {
      "name": "Inherited class",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c0cbad"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function"],
      "settings": {
        "fontStyle": "",
        "foreground": "#c0cbad"
      }
    },
    {
      "name": "Function argument",
      "scope": ["variable.parameter"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c5c8ba"
      }
    },
    {
      "name": "Tag name",
      "scope": ["entity.name.tag"],
      "settings": {
        "fontStyle": "",
        "foreground": "#b2d189"
      }
    },
    {
      "name": "Tag attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "fontStyle": "",
        "foreground": "#c0cbad"
      }
    },
    {
      "name": "Library function",
      "scope": ["support.function"],
      "settings": {
        "fontStyle": "",
        "foreground": "#c0cbad"
      }
    },
    {
      "name": "Library constant",
      "scope": ["support.constant"],
      "settings": {
        "fontStyle": "",
        "foreground": "#c0cbad"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "fontStyle": "",
        "foreground": "#c0cbad"
      }
    },
    {
      "name": "Library variable",
      "scope": ["support.other.variable"],
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "background": "#ffb4ab",
        "fontStyle": "",
        "foreground": "#690005"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "background": "#93000a",
        "foreground": "#ffdad6"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c0cbad"
      }
    },
    {
      "name": "Markup: Strong",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#c0cbad"
      }
    },
    {
      "name": "Markup: Underline",
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": ["markup.heading"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#b2d189"
      }
    },
    {
      "name": "Markup: Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#c0cbad"
      }
    }
  ]
}
