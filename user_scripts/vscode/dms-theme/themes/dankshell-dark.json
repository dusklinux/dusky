{
  "$schema": "vscode://schemas/color-theme",
  "name": "DankShell Dark - Generated",
  "type": "dark",
  "colors": {
    "editor.background": "#11140e",
    "editor.foreground": "#e1e4d9",
    "editor.lineNumberColor": "#c4c8bb",
    "editorLineNumber.foreground": "#c4c8bb",
    "editorLineNumber.activeForeground": "#e1e4d9",
    "editorCursor.foreground": "#acd28f",
    "editor.selectionBackground": "#2f4f1b60",
    "editor.wordHighlightBackground": "#3e4a3540",
    "editor.lineHighlightBackground": "#1d211a40",
    "editorWhitespace.foreground": "#43483e",
    "editorIndentGuide.foreground": "#43483e40",
    "editorError.foreground": "#ffb4ab",
    "editorWarning.foreground": "#bbeceb",
    "editorInfo.foreground": "#bdcbaf",
    "editorHint.foreground": "#a0cfce",
    "activityBar.background": "#191d16",
    "activityBar.foreground": "#c4c8bb",
    "activityBar.activeBorder": "#acd28f",
    "activityBarBadge.background": "#acd28f",
    "activityBarBadge.foreground": "#193705",
    "sideBar.background": "#191d16",
    "sideBar.foreground": "#c4c8bb",
    "sideBar.border": "#43483e20",
    "sideBarTitle.foreground": "#e1e4d9",
    "sideBarSectionHeader.background": "#1d211a",
    "sideBarSectionHeader.foreground": "#e1e4d9",
    "list.focusBackground": "#1d211a",
    "list.hoverBackground": "#1d211a40",
    "list.activeSelectionBackground": "#2f4f1b",
    "list.activeSelectionForeground": "#c7eea9",
    "list.inactiveSelectionBackground": "#282b24",
    "statusBar.background": "#33362f",
    "statusBar.foreground": "#e1e4d9",
    "statusBar.border": "#43483e20",
    "statusBar.debuggingBackground": "#93000a",
    "statusBar.debuggingForeground": "#ffdad6",
    "statusBarItem.hoverBackground": "#33362f80",
    "titleBar.activeBackground": "#0c0f09",
    "titleBar.activeForeground": "#e1e4d9",
    "titleBar.inactiveBackground": "#0c0f0980",
    "titleBar.inactiveForeground": "#c4c8bb",
    "commandCenter.background": "#1d211a",
    "commandCenter.foreground": "#e1e4d9",
    "commandCenter.border": "#43483e40",
    "tab.activeBackground": "#1d211a",
    "tab.activeForeground": "#e1e4d9",
    "tab.activeBorder": "#acd28f",
    "tab.inactiveBackground": "#191d16",
    "tab.inactiveForeground": "#c4c8bb",
    "tab.hoverBackground": "#1d211a40",
    "tab.border": "#43483e20",
    "tab.unfocusedActiveBackground": "#1d211a60",
    "tab.unfocusedActiveForeground": "#c4c8bb",
    "input.background": "#282b24",
    "input.foreground": "#e1e4d9",
    "input.border": "#43483e40",
    "input.placeholderForeground": "#c4c8bb",
    "inputOption.activeBackground": "#2f4f1b",
    "inputOption.activeForeground": "#c7eea9",
    "button.background": "#acd28f",
    "button.foreground": "#193705",
    "button.hoverBackground": "#acd28fdd",
    "button.secondaryBackground": "#bdcbaf",
    "button.secondaryForeground": "#283420",
    "button.secondaryHoverBackground": "#bdcbafdd",
    "dropdown.background": "#282b24",
    "dropdown.foreground": "#e1e4d9",
    "dropdown.border": "#43483e40",
    "progressBar.background": "#acd28f",
    "scrollbar.shadow": "#00000040",
    "scrollbarSlider.background": "#43483e40",
    "scrollbarSlider.hoverBackground": "#43483e60",
    "scrollbarSlider.activeBackground": "#43483e80",
    "editorGroup.border": "#43483e20",
    "editorGroup.dropBackground": "#2f4f1b40",
    "editorGroupHeader.tabsBackground": "#191d16",
    "editorGroupHeader.tabsBorder": "#43483e20",
    "diffEditor.insertedTextBackground": "#1e4e4e40",
    "diffEditor.removedTextBackground": "#93000a40",
    "diffEditor.diagonalFill": "#43483e20",
    "minimap.background": "#191d16",
    "minimap.selectionHighlight": "#2f4f1b60",
    "minimap.errorHighlight": "#ffb4ab60",
    "minimap.warningHighlight": "#bbeceb60",
    "peekView.border": "#43483e",
    "peekViewEditor.background": "#1d211a80",
    "peekViewEditor.matchHighlightBackground": "#3e4a3560",
    "peekViewResult.background": "#191d16",
    "peekViewResult.matchHighlightBackground": "#3e4a3540",
    "peekViewResult.selectionBackground": "#2f4f1b40",
    "widget.shadow": "#00000040",
    "problemsErrorIcon.foreground": "#ffb4ab",
    "problemsWarningIcon.foreground": "#bbeceb",
    "problemsInfoIcon.foreground": "#bdcbaf",
    "debugExceptionWidget.background": "#93000a",
    "debugExceptionWidget.border": "#ffb4ab",
    "debugToolBar.background": "#1d211a",
    "debugToolBar.border": "#43483e20",
    "breadcrumb.background": "#11140e",
    "breadcrumb.foreground": "#c4c8bb",
    "breadcrumb.focusForeground": "#e1e4d9",
    "breadcrumb.activeSelectionForeground": "#acd28f"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "foreground": "#c4c8bb",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#a0cfce"
      }
    },
    {
      "name": "String Escape",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#bdcbaf"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#bdcbaf"
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#bdcbaf"
      }
    },
    {
      "name": "Built-in Constant",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#bdcbaf"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#e1e4d9"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#acd28f"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage"],
      "settings": {
        "foreground": "#acd28f"
      }
    },
    {
      "name": "Storage Type",
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#acd28f"
      }
    },
    {
      "name": "Class name",
      "scope": ["entity.name.class"],
      "settings": {
        "fontStyle": "",
        "foreground": "#bdcbaf"
      }
    },
    {
      "name": "Inherited class",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#bdcbaf"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function"],
      "settings": {
        "fontStyle": "",
        "foreground": "#bdcbaf"
      }
    },
    {
      "name": "Function argument",
      "scope": ["variable.parameter"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c4c8bb"
      }
    },
    {
      "name": "Tag name",
      "scope": ["entity.name.tag"],
      "settings": {
        "fontStyle": "",
        "foreground": "#acd28f"
      }
    },
    {
      "name": "Tag attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "fontStyle": "",
        "foreground": "#bdcbaf"
      }
    },
    {
      "name": "Library function",
      "scope": ["support.function"],
      "settings": {
        "fontStyle": "",
        "foreground": "#bdcbaf"
      }
    },
    {
      "name": "Library constant",
      "scope": ["support.constant"],
      "settings": {
        "fontStyle": "",
        "foreground": "#bdcbaf"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "fontStyle": "",
        "foreground": "#bdcbaf"
      }
    },
    {
      "name": "Library variable",
      "scope": ["support.other.variable"],
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "background": "#ffb4ab",
        "fontStyle": "",
        "foreground": "#690005"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "background": "#93000a",
        "foreground": "#ffdad6"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#bdcbaf"
      }
    },
    {
      "name": "Markup: Strong",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#bdcbaf"
      }
    },
    {
      "name": "Markup: Underline",
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": ["markup.heading"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#acd28f"
      }
    },
    {
      "name": "Markup: Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#bdcbaf"
      }
    }
  ]
}
