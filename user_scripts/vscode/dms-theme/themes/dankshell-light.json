{
  "$schema": "vscode://schemas/color-theme",
  "name": "DankShell Light - Generated",
  "type": "light",
  "colors": {
    "editor.background": "#fef7ff",
    "editor.foreground": "#1d1a20",
    "editor.lineNumberColor": "#49454e",
    "editorLineNumber.foreground": "#49454e",
    "editorLineNumber.activeForeground": "#1d1a20",
    "editorCursor.foreground": "#6a548d",
    "editor.selectionBackground": "#ecdcff60",
    "editor.wordHighlightBackground": "#eadef740",
    "editor.lineHighlightBackground": "#f3ecf440",
    "editorWhitespace.foreground": "#cbc4cf",
    "editorIndentGuide.foreground": "#cbc4cf40",
    "editorError.foreground": "#ba1a1a",
    "editorWarning.foreground": "#ffd9e0",
    "editorInfo.foreground": "#645b70",
    "editorHint.foreground": "#7f525c",
    "activityBar.background": "#f8f1f9",
    "activityBar.foreground": "#49454e",
    "activityBar.activeBorder": "#6a548d",
    "activityBarBadge.background": "#6a548d",
    "activityBarBadge.foreground": "#ffffff",
    "sideBar.background": "#f8f1f9",
    "sideBar.foreground": "#49454e",
    "sideBar.border": "#cbc4cf20",
    "sideBarTitle.foreground": "#1d1a20",
    "sideBarSectionHeader.background": "#f3ecf4",
    "sideBarSectionHeader.foreground": "#1d1a20",
    "list.focusBackground": "#f3ecf4",
    "list.hoverBackground": "#f3ecf440",
    "list.activeSelectionBackground": "#ecdcff",
    "list.activeSelectionForeground": "#240e45",
    "list.inactiveSelectionBackground": "#ede6ee",
    "statusBar.background": "#e7e0e8",
    "statusBar.foreground": "#1d1a20",
    "statusBar.border": "#cbc4cf20",
    "statusBar.debuggingBackground": "#ffdad6",
    "statusBar.debuggingForeground": "#410002",
    "statusBarItem.hoverBackground": "#e7e0e880",
    "titleBar.activeBackground": "#ffffff",
    "titleBar.activeForeground": "#1d1a20",
    "titleBar.inactiveBackground": "#ffffff80",
    "titleBar.inactiveForeground": "#49454e",
    "commandCenter.background": "#f3ecf4",
    "commandCenter.foreground": "#1d1a20",
    "commandCenter.border": "#cbc4cf40",
    "tab.activeBackground": "#f3ecf4",
    "tab.activeForeground": "#1d1a20",
    "tab.activeBorder": "#6a548d",
    "tab.inactiveBackground": "#f8f1f9",
    "tab.inactiveForeground": "#49454e",
    "tab.hoverBackground": "#f3ecf440",
    "tab.border": "#cbc4cf20",
    "tab.unfocusedActiveBackground": "#f3ecf460",
    "tab.unfocusedActiveForeground": "#49454e",
    "input.background": "#ede6ee",
    "input.foreground": "#1d1a20",
    "input.border": "#cbc4cf40",
    "input.placeholderForeground": "#49454e",
    "inputOption.activeBackground": "#ecdcff",
    "inputOption.activeForeground": "#240e45",
    "button.background": "#6a548d",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#6a548ddd",
    "button.secondaryBackground": "#645b70",
    "button.secondaryForeground": "#ffffff",
    "button.secondaryHoverBackground": "#645b70dd",
    "dropdown.background": "#ede6ee",
    "dropdown.foreground": "#1d1a20",
    "dropdown.border": "#cbc4cf40",
    "progressBar.background": "#6a548d",
    "scrollbar.shadow": "#00000040",
    "scrollbarSlider.background": "#cbc4cf40",
    "scrollbarSlider.hoverBackground": "#cbc4cf60",
    "scrollbarSlider.activeBackground": "#cbc4cf80",
    "editorGroup.border": "#cbc4cf20",
    "editorGroup.dropBackground": "#ecdcff40",
    "editorGroupHeader.tabsBackground": "#f8f1f9",
    "editorGroupHeader.tabsBorder": "#cbc4cf20",
    "diffEditor.insertedTextBackground": "#ffd9e040",
    "diffEditor.removedTextBackground": "#ffdad640",
    "diffEditor.diagonalFill": "#cbc4cf20",
    "minimap.background": "#f8f1f9",
    "minimap.selectionHighlight": "#ecdcff60",
    "minimap.errorHighlight": "#ba1a1a60",
    "minimap.warningHighlight": "#ffd9e060",
    "peekView.border": "#cbc4cf",
    "peekViewEditor.background": "#f3ecf480",
    "peekViewEditor.matchHighlightBackground": "#eadef760",
    "peekViewResult.background": "#f8f1f9",
    "peekViewResult.matchHighlightBackground": "#eadef740",
    "peekViewResult.selectionBackground": "#ecdcff40",
    "widget.shadow": "#00000040",
    "problemsErrorIcon.foreground": "#ba1a1a",
    "problemsWarningIcon.foreground": "#ffd9e0",
    "problemsInfoIcon.foreground": "#645b70",
    "debugExceptionWidget.background": "#ffdad6",
    "debugExceptionWidget.border": "#ba1a1a",
    "debugToolBar.background": "#f3ecf4",
    "debugToolBar.border": "#cbc4cf20",
    "breadcrumb.background": "#fef7ff",
    "breadcrumb.foreground": "#49454e",
    "breadcrumb.focusForeground": "#1d1a20",
    "breadcrumb.activeSelectionForeground": "#6a548d"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "foreground": "#49454e",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#7f525c"
      }
    },
    {
      "name": "String Escape",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#645b70"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#645b70"
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#645b70"
      }
    },
    {
      "name": "Built-in Constant",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#645b70"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#1d1a20"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#6a548d"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage"],
      "settings": {
        "foreground": "#6a548d"
      }
    },
    {
      "name": "Storage Type",
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#6a548d"
      }
    },
    {
      "name": "Class name",
      "scope": ["entity.name.class"],
      "settings": {
        "fontStyle": "",
        "foreground": "#645b70"
      }
    },
    {
      "name": "Inherited class",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#645b70"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function"],
      "settings": {
        "fontStyle": "",
        "foreground": "#645b70"
      }
    },
    {
      "name": "Function argument",
      "scope": ["variable.parameter"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#49454e"
      }
    },
    {
      "name": "Tag name",
      "scope": ["entity.name.tag"],
      "settings": {
        "fontStyle": "",
        "foreground": "#6a548d"
      }
    },
    {
      "name": "Tag attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "fontStyle": "",
        "foreground": "#645b70"
      }
    },
    {
      "name": "Library function",
      "scope": ["support.function"],
      "settings": {
        "fontStyle": "",
        "foreground": "#645b70"
      }
    },
    {
      "name": "Library constant",
      "scope": ["support.constant"],
      "settings": {
        "fontStyle": "",
        "foreground": "#645b70"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "fontStyle": "",
        "foreground": "#645b70"
      }
    },
    {
      "name": "Library variable",
      "scope": ["support.other.variable"],
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "background": "#ba1a1a",
        "fontStyle": "",
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "background": "#ffdad6",
        "foreground": "#410002"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#645b70"
      }
    },
    {
      "name": "Markup: Strong",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#645b70"
      }
    },
    {
      "name": "Markup: Underline",
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": ["markup.heading"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#6a548d"
      }
    },
    {
      "name": "Markup: Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#645b70"
      }
    }
  ]
}
